{
  "manifest_version": 2,
  "name": "Onemark",
  "shortname": "Onemark",
  "version": "1.0.5",
  "description": "Aggregate and manage browser working session",
  "applications": {
    "gecko": {
      "id": "onemark@waelrabadi.com"
    }
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Onemark"
  },
  "icons": {
    "48": "icon.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "notifications",
    "nativeMessaging",
    "storage"
  ]
}